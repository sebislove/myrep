<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>mavecontroller</servlet-name>
  	<display-name>mavecontroller</display-name>
  	<description></description>
  	<servlet-class>com.maven.controller.mavecontroller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>aaa</servlet-name>
  	<display-name>aaa</display-name>
  	<description></description>
  	<servlet-class>com.cq.controller.aaa</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>mavecontroller</servlet-name>
  	<url-pattern>/mavecontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>aaa</servlet-name>
  	<url-pattern>/aaa</url-pattern>
  </servlet-mapping>
</web-app>
